<h3>User Summary: <%=link_to @user.name %></h3>
<p>
  Joined site <%@user.created_at%>
</p>
<h4>Votes</h4>
<table>
  <tr>
    <th>Media Title</th>
    <th>Created by</th>
    <th>Published</th>
    <th>Category</th>
    <th>Voted On</th>
  </tr>
  <% @user.upvotes.each do |work_id| %>
  <% work = Work.find(work_id.work_id)%>
  <tr>
    <td><%=link_to work.title, work_path(work.id)%></td>
    <td><%=work.creator%></td>
    <td><%=work.publication_year%></td>
    <td><%= work.category %></td>
    <td><%=work_id.created_at%></td>
  </tr>
  <% end %>
</table>
