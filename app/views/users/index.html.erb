<main class="row">
   <h3>List of Users</h3>
   <table>
     <thead>
       <tr>
         <th>Username</th>
         <th width="100">Votes</th>
         <th width="200">Joined</th>
       </tr>
     </thead>
     <tbody>
       <% @users.each do |user| %>
       <tr>
         <td>
           <%= "#{user.username}" %>
         </td>
         <td>
           <%= "Future Votes" %>
         </td>
         <td>
           <%= "#{user.created_at.to_date}" %>
         </td>
       </tr>
       <% end %>
     </tbody>
   </table>

   <%= link_to "Back to Media List", root_path, class: :button %>
</main>
