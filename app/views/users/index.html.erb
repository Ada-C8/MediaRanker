<h3>List of Users</h3>

<table>
  <tr>
    <th>Username</th>
    <th width="100">Votes</th>
    <th width="200">Joined</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= link_to user.name, user_path(user.id) %></td>
    <td><%= user.votes.size %></td>
    <td><%= user.created_at.strftime('%b %d, %Y') %></td>
  </tr>
  <% end %>
</table>
