<section class="row">
  <h3>List of Users</h3>
  <table>
    <tr>
      <th>Username</th>
      <th width="100">Votes</th>
      <th width="100">Joined</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user.id) %></td>
      <td><%= user.votes.count %> </td>
      <td><%= user.created_at %></td>
    </tr>
    <% end %>
  </table>
</section>
