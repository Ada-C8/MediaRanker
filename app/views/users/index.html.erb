<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<section>
    <% @users.each do |user| %>
    <article>
      <p>Username: <%= link_to(user.username, user_path(user.id)) %></p>
      <p>Votes:</p>
      <p>Joined: <%= user.created_at.strftime("%b %d, %Y") %></p>
      <hr>
    </article>
    <% end %>
</section>
