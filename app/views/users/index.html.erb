<h2>List of Users</h2>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id) %></td>
      <td><%= user.votes.count %></td>
      <td><%= user.created_at.strftime("%b %d,%Y") %></td>
    </tr>
  </tbody>
  <% end %>

</table>
