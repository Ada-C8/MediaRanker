<main class='row'>
  <h3> List of Users </h3>
  <table>
    <thead>
      <th> Username </th>
      
      <th width = '200'> Joined </th>
    </thead>

    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td> <%= link_to user.username, user_path(user) %> </td>
        <td><%= user.created_at.strftime("%B %d, %Y")%></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to 'Back to Media List', root_path, :class => 'button' %>
</main>
