<h3>List of Users</h3>
<table>
  <thead>
    <tr>
      <td>Username</td>
      <td>Votes</td>
      <td>Joined</td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user.id)%></td>
        <td width="100"><%= user.votes.count%></td>
        <td width="200"><%= user.created_at.to_date%></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Back to Media List", works_path, class: "button"%>
