<h3>List of Users</h3>
<table>
  <thead>
    <tr>
      <th>
        Username
      </th>
      <th>
        Votes
      </th>
      <th>
        Joined
      </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%=link_to(user.name, user_path(user.id)) %>
      </td>
      <td>
        <%= user.votes.length %>
      </td>
      <td>
        <%= date_format(user.created_at)%>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
