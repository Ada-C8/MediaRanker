<table>
  <thead>
    <tr> Votes </tr>
    <tr> Title </tr>
    <tr> Created By </tr>
    <tr> Published </tr>
    <tr> Upvote </tr>
  </thead>
  <tbody>
  <% works.each do |work| %>
  <tr>
    <td><%= work.votes.count %></td>
    <td><%= work.title %></td>
    <td><%= work.creator %></td>
    <td><%= work.publication_year %></td>
    <td><%= link_to("Upvote", votes_path(:work => work.id), method: :post )%></td>
  </tr>
  <% end %>
  </tbody>
</table>
