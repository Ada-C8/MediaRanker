<h1>Works#index</h1>
<p>Find me in app/views/works/index.html.erb</p>

<% @media.each do |type, top_ten| %>
  <section class="<%=type%>">
    <h1> Top <%=type.capitalize%></h1>

    <% top_ten.each do |work| %>
    <ul>
      <li>
        <%= "#{work.title} by #{work.creator}" %>
      </li>
    </ul>
    <% end %>
  </section>

<% end %>
