<h4><%= model.first.category.capitalize%>s</h4>
<table>
  <thead>
    <tr>
      <th>Votes</th>
      <th>Title</th>
      <th>Created By</th>
      <th>Published</th>
      <th>Upvote</th>
    </tr>
  </thead>
  <tbody>
    <% model.each do |work| %>
      <tr>
        <td><%= work.votes.count %></td>
        <td><%= link_to work.title, work_path(work.id) %></td>
        <td><%= work.creator %></td>
        <td><%= work.publication_year %></td>
        <td>upvote link here</td>
      </tr>
    <% end %>
  </tbody>
</table>
