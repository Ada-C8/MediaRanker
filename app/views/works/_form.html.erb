<%= form_for @work do |f| %>
  <p>
    <%= f.label :category%>
    <%= f.text_field :category %>
  </p>

  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :creator %>
    <%= f.text_field :creator %>
  </p>

  <p>
    <%= f.label :publication_year %>
    <%= f.text_field  :publication_year %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>


  <%= f.submit some_var %>
<% end %>
