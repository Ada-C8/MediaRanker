<ul class= "form">
  <%= form_for @work do |f| %>
    <li>
      <%= f.label :category%>
      <%= f.text_field :category %>
      <%# f.collection_select(:category)%>
    </li>
    <li>
      <%= f.label :title%>
      <%= f.text_field :title%>
    </li>

    <li>
      <%= f.label :creator %>
      <%= f.text_field :creator %>
    </li>
    <li>
      <%= f.label :publication_year %>
      <%= f.text_field :publication_year %>
    </li>

    <li>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </li>

    <li>
      <%= f.submit %>
    </li>
  <% end %>
</ul>
